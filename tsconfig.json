{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "api/*": ["src/api/*"],
      "reduxApp/*": ["src/redux/*"],
      "pages/*": ["src/pages/*"],
      "components/*": ["src/components/*"]
    },
    "target": "es5",
    "module": "commonjs",
    "allowJs": true,
    "jsx": "react",
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true
  },
  "exclude": ["node_modules", "dist"]
}
